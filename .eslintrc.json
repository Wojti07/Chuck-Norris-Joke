{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eol-last": ["error", "never"],
        "semi": ["error", "always"],
        "space-before-function-paren": ["error", "never"],
        "quotes": "off",
        "no-tabs": 0
    }
    
}
